<script>
  $(function() {
  // 置換の対象とするclass属性。
  var $elem = $('.js-image-switch');
  // 置換の対象とするsrc属性の末尾の文字列。
  var sp = '_sp.';
  var pc = '_pc.';
  // 画像を切り替えるウィンドウサイズ。
  var replaceWidth = 768;

  function imageSwitch() {
    // ウィンドウサイズを取得する。
    var windowWidth = parseInt($(window).width());

    // ページ内にあるすべての`.js-image-switch`に適応される。
    $elem.each(function() {
      var $this = $(this);
      // ウィンドウサイズが768px以上であれば_spを_pcに置換する。
      if(windowWidth >= replaceWidth) {
        $this.attr('src', $this.attr('src').replace(sp, pc));

      // ウィンドウサイズが768px未満であれば_pcを_spに置換する。
      } else {
        $this.attr('src', $this.attr('src').replace(pc, sp));
      }
    });
  }
  imageSwitch();

  // 動的なリサイズは操作後0.2秒経ってから処理を実行する。
  var resizeTimer;
  $(window).on('resize', function() {
    clearTimeout(resizeTimer);
    resizeTimer = setTimeout(function() {
      imageSwitch();
    }, 200);
  });



  if ($('.until_trial_end').length > 0) {
    setInterval(countDown, 1000);
    function countDown() {
      var hour = $($('.until_trial_end .end_hour')[0]).text()
      var minute = $($('.until_trial_end .end_minute')[0]).text()
      var second = $($('.until_trial_end .end_second')[0]).text()
      var all = (parseInt(hour) * 60 * 60) + (parseInt(minute) * 60) + parseInt(second);
      if (all <= 0) {
        return;
      }
      all -= 1
      var next_hour = all / (60*60)
      var spare_hour = all % (60*60)
      var next_minute = spare_hour / (60)
      var spare_minute = all % 60
      var next_second = spare_minute
      var next_hour_str = String(parseInt(next_hour));
      if (parseInt(next_hour) < 10) {
        next_hour_str = "0" + next_hour_str;
      }
      var next_minute_str = String(parseInt(next_minute));
      if (parseInt(next_minute) < 10) {
        next_minute_str = "0" + next_minute_str;
      }
      var next_second_str = String(parseInt(next_second));
      if (parseInt(next_second) < 10) {
        next_second_str = "0" + next_second_str;
      }
      $('.until_trial_end .end_second').text(next_second_str);
      $('.until_trial_end .end_minute').text(next_minute_str);
      $('.until_trial_end .end_hour').text(next_hour_str);
    }
  }

  $('.apply_box .min_tit, .apply_box .tokuten_kikan,.apply_box .price_box').click(function() {
    $(this).parent().find('.btn-apply')[0].click();

  })


  });
</script>

<script>
  Admin.setPage('common');
  Admin.init();
</script>
</body>
</html>
