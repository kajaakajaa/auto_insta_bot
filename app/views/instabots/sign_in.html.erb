<script>
  $(function() {
      $('#insta_auth_form').submit(function() {
          var user_name = $('#insta_username').val();
          if (user_name.match(/^[a-zA-Z0-9_.]+$/) == null) {
                $('.group-insta-id').addClass('has-error');
              $('.error-field').removeClass('hide');
              $('html,body').animate({scrollTop: top}, 0, 'swing');
              return false;
          } else {
              return true;
          }

      });
  });
</script>

